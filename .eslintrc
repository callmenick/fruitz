{
  extends: "eslint:recommended",
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "globals": {
  },
  rules: {
    "no-console": 0,
    "semi": [2, "always"],
    "comma-spacing": [2, {
      "before": false,
      "after": true
    }],
    "object-curly-spacing": [2, "never"],
    "computed-property-spacing": [2, "never"],
    "space-in-parens": [2, "never"],
    "eol-last": 1,
    "key-spacing": [2, {
      "beforeColon": false,
      "afterColon": true,
      "mode": "strict"
    }],
    "no-multiple-empty-lines": [2, {
      "max": 1,
      "maxEOF": 1
    }],
    "no-unneeded-ternary": 2,
    "padded-blocks": [2, "never"],
    "quotes": [2, "single"],
    "comma-style": 2,
    "no-extra-parens": 2,
    "valid-jsdoc": [2, {
      "prefer": {
          "return": "return"
      },
      "requireReturn": false
    }]
  }
}
